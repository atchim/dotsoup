def_key "ctrl-d"
  page_down

def_key "ctrl-u"
  page_up

def_key "d"
  delete_playlist_items

def_key "d"
  delete_browser_items

def_key "d"
  delete_stored_playlist

def_key "G"
  move_end

def_key "g"
  move_home

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "J"
  select_item
  scroll_down

def_key "j"
  scroll_down

def_key "K"
  select_item
  scroll_up

def_key "k"
  scroll_up

def_key "l"
  next_column

def_key "l"
  slave_screen
